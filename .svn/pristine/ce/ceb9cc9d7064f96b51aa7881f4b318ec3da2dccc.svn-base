INFT\prosgaBundle\Entity\RegistroFP:
    type: entity
    table: RegistroFP
    fields:
        id:
            type: integer
            id: true
            generator:
                strategy: AUTO
        codigoDeFicha:
            type: string
            length: 255
            column: CodigoDeFicha
        nombre:
            type: string
            length: 255
            column: Nombre
        ubicacionArchivoFisico:
            type: string
            length: 255
            column: UbicacionArchivoFisico
        ubicacionArchivoDigital:
            type: string
            length: 255
            column: UbicacionArchivoDigital
        tiempoEnArchivo:
            type: datetime
            column: TiempoEnArchivo
        observacion:
            type: string
            length: '1000'
            column: Observacion
            
    ManyToOne:
        FichaDeProceso:
            targetEntity: FichaDeProceso
            joinColumn: 
                name: IdFichaDeProceso
                referencedColumnName: id
    lifecycleCallbacks: {  }
