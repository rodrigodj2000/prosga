{% extends 'prosgaBundle::template.html.twig' %}

{% block content %}
    <h2>Ver Salidas de Procesos</h2>
{% endblock %}

{% block body_content -%}
    <table class="table">
        <tbody>
            <tr>
                <th> Salida del Proceso </th>
                <td><a href="{{ path('fichadeproceso_show', { 'id': entity.fichaDeProceso.id }) }}">{{ entity.fichaDeProceso}} </a>
            </tr>
            <tr>
                <th>Valores de Salida</th>
                <td>{{ entity.valoresDeSalida }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ entity.observacion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('fichadeprocesosalida') }}">
            Volver al Listado
        </a>
    </li>
    <li>
        <a href="{{ path('fichadeprocesosalida_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
