{% extends 'prosgaBundle::template.html.twig' %}

{% block content %}
    <h2>Ver Entradas de Procesos</h2>
{% endblock %}

{% block body_content -%}
    <table class="table">
        <tbody>
            <tr>
                <th> Entrada del Proceso </th>
                <td><a href="{{ path('fichadeproceso_show', { 'id': entity.fichaDeProceso.id }) }}">{{ entity.fichaDeProceso}} </a>
            </tr>
            <tr>
                <th>Valores de Entrada</th>
                <td>{{ entity.valoresDeEntrada }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entity.observacion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('fichadeprocesoentrada') }}">
            Volver a la Lista
        </a>
    </li>
    <li>
        <a href="{{ path('fichadeprocesoentrada_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
